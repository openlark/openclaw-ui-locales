{
  "name": "openclaw-ui-locales",
  "version": "0.3.1",
  "description": "A multilingual switching tool specifically designed for the OpenClaw Web control interface. It allows you to seamlessly switch between over 10 languages with a single click.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "locale": "bun run ./scripts/index.ts",
    "openclaw": "bun run ./scripts/openclaw.ts",
    "build": "bun build ./bin/cli.ts --outfile ./dist/index.js --target node --minify"
  },
  "bin": {
    "openclaw-ui": "./dist/index.js"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "src"
  ],
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "commander": "^14.0.3",
    "open": "^11.0.0",
    "shelljs": "^0.10.0",
    "latest-version": "^9.0.0",
    "compare-version": "^0.1.2"
  },
  "devDependencies": {
    "bun": "^1.3.9",
    "@types/bun": "^1.3.9",
    "@types/node": "^25.2.3",
    "@types/shelljs": "^0.10.0",
    "@types/latest-version": "^4.0.1",
    "@types/compare-version": "^0.1.34"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openlark/openclaw-ui-locales.git"
  },
  "bugs": {
    "url": "https://github.com/openlark/openclaw-ui-locales/issues"
  },
  "homepage": "https://github.com/openlark/openclaw-ui-locales#readme",
  "keywords": [
    "openclaw-ui-locales",
    "openclaw",
    "locales"
  ],
  "license": "MIT"
}